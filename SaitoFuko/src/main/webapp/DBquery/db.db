//user_info
create table user_info(
user_id varchar2(30) PRIMARY KEY,
password varchar2(30),
email varchar2(50),
user_name varchar2(15),
gender varchar2(10),
nickname varchar2(30),
birthday varchar2(20),
user_stat varchar2(10)
)


//emoticon_info
create table emoticon_info(
emo_id varchar2(10) primary key,
emo_name varchar2(20),
emo_info varchar2(1000),
emo_price number(10),
emo_picture varchar2(100)
)

//have_emoticon
create table have_emoticon(
user_id varchar2(30) REFERENCES user_info (user_id),
emo_id varchar2(10) REFERENCES emoticon_info (emo_id)
)

//emo_cart
create table emo_cart(
user_id varchar2(30) REFERENCES user_info (user_id),
emo_id varchar2(10) REFERENCES emoticon_info (emo_id)
)

//emoticon_sale
create table emoticon_sale(
emo_order_code varchar2(10) primary key,
user_id varchar2(30) REFERENCES user_info (user_id),
price number(10),
order_date varchar2(20),
emo_id varchar2(10) REFERENCES emoticon_info (emo_id)
)

//refund_emoticon
create table refund_emoticon(
emo_order_code varchar2(10) REFERENCES emoticon_sale (emo_order_code),
refund_date varchar2(20)
)
//board
create table board(
board_id number(10) primary key,
board_name varchar2(20),
board_grade number(2),
board_location number(2) UNIQUE
)


//post
create table post(
post_no number(10) primary key,
user_id varchar2(30) REFERENCES user_info (user_id),
title varchar2(30),
content long,
post_date varchar2(20),
origin_no number(10),
group_ord number(10),
groupLayer number(10),
board_id number(10)  REFERENCES board (board_id),
post_views number(10)
)

create table comment_post(
comment_no number(10) primary key,
post_no number(10) REFERENCES post (post_no),
user_id varchar2(30) REFERENCES user_info (user_id),
comment_date varchar2(20),
comment_parent number(10),
comment_content varchar2(1000),
emo_id varchar2(10) REFERENCES emoticon_info (emo_id)
)





